<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.custom.dao.ProductImageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.custom.model.ProductImage">
        <id column="productKey" property="productKey" />
        <result column="fileName" property="fileName" />
        <result column="contentType" property="contentType" />
        <result column="fileKey" property="fileKey" />
        <result column="fileRealPath" property="fileRealPath" />
        <result column="fileSize" property="fileSize" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        productKey, fileName, contentType, fileKey, fileRealPath, fileSize
    </sql>

</mapper>
